    "def canMakeArithmeticProgression(arr):\n",
    "    arr.sort()\n",
    "    diff = arr[1] - arr[0]\n",
    "    for i in range(1, len(arr) - 1):\n",
    "        if arr[i + 1] - arr[i] != diff:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "print(canMakeArithmeticProgression([1,2,4]))"
   ]
