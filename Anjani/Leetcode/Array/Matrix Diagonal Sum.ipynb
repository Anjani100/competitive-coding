    "def diagonalSum(mat):\n",
    "    s = 0\n",
    "    n = len(mat)\n",
    "    for i in range(n):\n",
    "        s += mat[i][i]\n",
    "        if i != n - i - 1:\n",
    "            s += mat[n - i - 1][i]\n",
    "    return s\n",
    "\n",
    "print(diagonalSum([[5]]))"
   ]
