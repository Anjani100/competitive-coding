    "def containsNearbyDuplicate(nums, k):\n",
    "    dict_nums = {}\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i] in dict_nums and abs(i - dict_nums[nums[i]]) <= k:\n",
    "            return True\n",
    "        dict_nums[nums[i]] = i\n",
    "    return False\n",
    "print(containsNearbyDuplicate([1, 2, 3, 4, 5, 6], 3))"
   ]
