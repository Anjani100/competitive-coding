    "def sortedSquares(A):\n",
    "    for i in range(len(A)):\n",
    "        A[i] *= A[i]\n",
    "    A.sort()\n",
    "    return A\n",
    "\n",
    "print(sortedSquares([-7,-3,2,3,11]))"
   ]
