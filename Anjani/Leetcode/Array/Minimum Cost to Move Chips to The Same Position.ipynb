   "source": [
    "def minCostToMoveChips(position):\n",
    "    s = set(position)\n",
    "    a = b = 0\n",
    "    for i in s:\n",
    "        if i % 2 == 0:\n",
    "            a += position.count(i)\n",
    "        else:\n",
    "            b += position.count(i)\n",
    "    \n",
    "    return min(a, b)\n",
    "\n",
    "print(minCostToMoveChips([1,2,3]))"
   ]
