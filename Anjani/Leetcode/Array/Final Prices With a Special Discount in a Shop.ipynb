    "def finalPrices(prices):\n",
    "    for i in range(len(prices)):\n",
    "        for j in range(i + 1, len(prices)):\n",
    "            if prices[j] <= prices[i]:\n",
    "                prices[i] -= prices[j]\n",
    "                break\n",
    "    return prices\n",
    "\n",
    "print(finalPrices([10,1,1,6]))"
   ]
