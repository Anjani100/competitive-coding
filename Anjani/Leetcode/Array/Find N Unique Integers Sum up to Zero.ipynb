   "source": [
    "def sumZero(n):\n",
    "    res = []\n",
    "    a = n // 2\n",
    "    i = -1 * a\n",
    "    while i <= a:\n",
    "        res.append(i)\n",
    "        i += 1\n",
    "    if n % 2 == 0:\n",
    "        res.remove(0)\n",
    "    return res\n",
    "\n",
    "print(sumZero(3))"
   ]
