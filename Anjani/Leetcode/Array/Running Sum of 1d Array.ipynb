   "source": [
    "def runningSum(nums):\n",
    "    s = sum(nums)\n",
    "    res = [s]\n",
    "    for i in range(len(nums) - 2, -1, -1):\n",
    "        s -= nums[i + 1]\n",
    "        res.append(s)\n",
    "    res.reverse()\n",
    "    return res\n",
    "\n",
    "print(runningSum([3,1,2,10,1]))"
   ]
