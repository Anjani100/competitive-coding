    "def shuffle(nums, n):\n",
    "    res = []\n",
    "    for i in range(n):\n",
    "        res.append(nums[i])\n",
    "        res.append(nums[n + i])\n",
    "    return res\n",
    "\n",
    "print(shuffle([1,2], 1))"
   ]
